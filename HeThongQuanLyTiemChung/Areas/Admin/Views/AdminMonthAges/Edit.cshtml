@model HeThongQuanLyTiemChung.Models.MonthAge

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<div class="page-header">
    <div class="header-sub-title">
        <nav class="breadcrumb breadcrumb-dash">
            <a asp-area="Admin" asp-controller="Home" asp-action="Index" class="breadcrumb-item"><i class="anticon anticon-home m-r-5"></i>Trang chủ</a>
            <a asp-area="Admin" asp-controller="AdminMonthAges" asp-action="Index" class="breadcrumb-item">Danh sách tháng tuổi</a>
            <span class="breadcrumb-item active">Chỉnh sửa tháng tuổi</span>
        </nav>
    </div>
</div>


<form asp-action="Edit" asp-route-id="@Model.MonthAgeId" method="post">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <input type="hidden" asp-for="MonthAgeId" />
    <div class="form-group">
        <div class="form-group col-md-6">
            <label class="col-sm-6 col-form-label">Tên tháng tuổi</label>
            <div class="col-sm-6">
                <input type="text" class="form-control" asp-for="MonthAgeName" placeholder="Tên tháng tuổi">
            </div>
        </div>

        <div class="form-group col-md-6">
            <div class="col-sm-6">
                <button type="submit" class="btn btn-primary">Lưu thay đổi</button>
            </div>
        </div>
    </div>
</form>


